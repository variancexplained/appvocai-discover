    "from dataclasses import dataclass\n",
    "from discover.core.data_class import DataClass\n",
    "\n",
    "\n",
    "@dataclass\n",
    "class A(DataClass):\n",
    "    a: str = \"a_something\"\n",
    "\n",
    "\n",
    "@dataclass\n",
    "class B(DataClass):\n",
    "    a: A = A()\n",
    "    b: int = 5\n",
    "\n",
    "\n",
    "b = B()\n",
    "print(b.as_df())"
